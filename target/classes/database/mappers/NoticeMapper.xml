<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
  <mapper namespace="com.example.demo.notice.NoticeRepository">
  	<select id="getList" parameterType="NoticeVO" resultType="NoticeVO">
  		select num, title, writer, hit, date from destudynotice
  	</select>
  	
  	<select id="getSelect" parameterType="NoticeVO" resultType="NoticeVO">
  		select * from destudynotice where num=#{num}
  	</select>
  </mapper>
  
